{
  "manifest_version": 2,
  "name": "Code Snippet Autocomplete",
  "version": "1.0",

  "description": "Insert code snippet.",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "web_accessible_resources": [
    "search.html"
  ],
  "permissions": ["tabs"]
}